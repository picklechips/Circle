{{#if error}}
	<div class="container">
		<div class="row"></div>
		<div class="center red white-text z-depth-1">{{error}}</div>
	</div>
{{/if}}

{{#each errors}}
	<div class="container">
		<div class="row"></div>
		<div class="center red white-text z-depth-1">{{text}}</div>
	</div>
{{else}}

{{/each}}