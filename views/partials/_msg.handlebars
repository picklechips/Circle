{{#if success_msg}}
	<div class="container">
		<div class="row"></div>
		<div class="center green white-text z-depth-1">{{success_msg}}</div>
	</div>{{/if}}

{{#if error_msg}}
	<div class="container">
		<div class="row"></div>
		<div class="center red white-text z-depth-1">{{error_msg}}</div>
	</div>{{/if}}